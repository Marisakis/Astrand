<Window x:Class="HealthcareDoctor.ClientHistoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HealthcareDoctor"
        xmlns:uicontrols="clr-namespace:UIControls;assembly=UIControls"
        xmlns:charts="clr-namespace:UIControls.Charts;assembly=UIControls"
        mc:Ignorable="d"
        Title="ClientHistoryWindow" Height="450" Width="800" Background="#FF1A1A1A">
    <Grid>
        <uicontrols:Container x:Name="con_ClientData" Header="Cliënt data" Grid.Column="2" Grid.Row="1" Grid.ColumnSpan="1" Background="#FF1E1E1E" Foreground="#FF333337" HeaderForeground="White" Margin="2.5,2.5,5,5">
            <Grid x:Name="grd_Grid">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
            </Grid>
        </uicontrols:Container>
    </Grid>
</Window>
